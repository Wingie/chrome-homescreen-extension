{
	"background_page": "html/background.html",
	"browser_action":  {"default_icon": "images/popup/icon.png", "default_title": "Homescreen Extension", "popup": "popup/popup.html"},
  	"description":     "Simple, useful homescreen extension. Appears when you open a new tab or window.",
	"icons":           {"16": "images/icons/16.png", 
                            "32": "images/icons/32.png",
                            "48": "images/icons/48.png", 
                            "64": "images/icons/64.png", 
                            "128": "images/icons/128.png"},
  	"name":	           "Homescreen Extension",
  	"options_page":	   "html/options.html",
  	"version":         "0.2",
	"permissions":	   ["tabs", "bookmarks", "http://*.google.com/", "https://*.google.com/"],

	"chrome_url_overrides": {
		"newtab": "html/newtab.html"
	}
}
